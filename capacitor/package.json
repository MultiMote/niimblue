    "@capacitor/filesystem": "^7.1.6",
    "@capacitor/ios": "^7.4.5",
    "@capacitor/share": "^7.0.3",
    "@capacitor/splash-screen": "^7.0.4"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.4.5"
  },

